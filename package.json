{
  "name": "lalocord",
  "version": "2.0.7",
  "description": "P2P streaming and chating",
  "author": {
    "name": "Nikita Ozhegov",
    "email": "nikita.ozhegov8@mail.ru"
  },
  "main": "dist-electron/main.js",
  "engines": {
    "node": ">=22.12.0",
    "npm": ">=10.9.0"
  },
  "repository": "github:Kaptoiiika/lalocord",
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "install:electron": "npm i --include=optional",
    "electron:dev": "vite --mode electron",
    "electron:build": "vite build --mode electron && electron-builder",
    "electron:preview": "vite build --mode electron && electron .",
    "test": "vitest",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "stylelint:fix": "stylelint --fix **/*.scss",
    "prettier": "prettier src/**/*.{js,jsx,ts,tsx,css,md,json} --check",
    "prettier:fix": "prettier src/**/*.{js,jsx,ts,tsx} --write ",
    "prettier-lint": "npm run prettier && npm run lint",
    "format": "npm run prettier:fix && npm run lint:fix"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.1",
    "@mui/material": "^7.3.1",
    "axios": "^1.11.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-linkify": "^1.0.0-alpha",
    "react-router-dom": "^7.8.1",
    "socket.io-client": "^4.8.1",
    "swr": "^2.3.6",
    "web-vitals": "^5.1.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/compat": "^1.3.2",
    "@stylistic/eslint-plugin": "^5.2.3",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.11",
    "@types/react-dom": "^19.1.7",
    "@types/react-linkify": "^1.0.4",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "@vitejs/plugin-react": "^5.0.1",
    "css-loader": "^7.1.2",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "file-loader": "^6.2.0",
    "prettier": "^3.6.2",
    "sass": "1.90.0",
    "typescript": "^5.9.2",
    "vite": "^7.1.3",
    "vite-plugin-checker": "^0.10.2",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-svgr": "^4.3.0",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "optionalDependencies": {
    "@electron/rebuild": "^4.0.2",
    "electron": "^39.2.7",
    "electron-builder": "^26.0.12",
    "electron-context-menu": "^4.1.1",
    "vite-plugin-electron": "^0.29.0",
    "vite-plugin-electron-renderer": "^0.14.6"
  },
  "overrides": {
    "vite": {
      "rollup": "npm:@rollup/wasm-node"
    }
  },
  "build": {
    "appId": "com.lalocord.app",
    "productName": "Lalocord",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "dist-electron/**/*",
      "!**/node_modules/**"
    ],
    "asarUnpack": [],
    "publish": {
      "provider": "github",
      "owner": "Kaptoiiika",
      "repo": "lalocord"
    },
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "public/logo512.png"
    },
    "linux": {
      "target": [
        "AppImage"
      ],
      "icon": "public/logo512.png"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
