{
  "name": "lalocord",
  "version": "1.4.3",
  "description": "P2P streaming and chating",
  "author": {
    "name": "Nikita Ozhegov",
    "email": "nikita.ozhegov8@mail.ru"
  },
  "main": ".webpack/main",
  "repository": "github:Kaptoiiika/lalocord",
  "scripts": {
    "start": "vite --mode development",
    "build": "vite build --emptyOutDir",
    "electron": "electron-forge start",
    "package": "electron-forge package",
    "publish": "electron-forge publish",
    "make": "electron-forge make",
    "lint": "npm run lint:ts && npm run lint:scss",
    "lint:fix": "npm run lint:ts:fix && npm run lint:scss:fix",
    "lint:ts": "npx eslint \"**/*{ts,tsx}\"",
    "lint:ts:fix": "npx eslint \"**/*{ts,tsx}\" --fix",
    "lint:scss": "npx stylelint \"**/*.scss\"",
    "lint:scss:fix": "npx stylelint \"**/*.scss\" --fix",
    "storybook": "start-storybook -p 6006 -c ./config/storybook",
    "build-storybook": "build-storybook -c ./config/storybook",
    "test:unit": "jest --config ./config/jest/jest.config.ts",
    "test:ui": "npx loki test --port 6006 ",
    "test:ui:ci": "npm run build-storybook && npx loki --requireReference --reactUri file:./storybook-static",
    "test:ui:ok": "npx loki approve",
    "test:ui:report": "npm run test:ui:report:json && npm run test:ui:report:html",
    "test:ui:report:json": "node scripts/generate-visual-json-report.js",
    "test:ui:report:html": "npx reg-cli --from .loki/report.json --report .loki/report.html",
    "prepare": "husky install",
    "dependency": "npx depcruise --config ./scripts/.dependency-cruiser.js src"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.1.1",
    "@electron-forge/maker-deb": "^6.1.1",
    "@electron-forge/maker-rpm": "^6.1.1",
    "@electron-forge/maker-squirrel": "^6.1.1",
    "@electron-forge/maker-zip": "^6.1.1",
    "@electron-forge/plugin-auto-unpack-natives": "^6.1.1",
    "@electron-forge/plugin-vite": "^6.2.1",
    "@electron-forge/plugin-webpack": "^6.2.1",
    "@electron-forge/publisher-github": "^6.2.1",
    "@feature-sliced/eslint-config": "^0.1.0-beta.6",
    "@storybook/addon-actions": "^6.5.12",
    "@storybook/addon-essentials": "^6.5.12",
    "@storybook/addon-interactions": "^6.5.12",
    "@storybook/addon-links": "^6.5.12",
    "@storybook/react": "^6.5.12",
    "@storybook/testing-library": "^0.0.13",
    "@svgr/webpack": "^8.0.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/circular-dependency-plugin": "^5.0.5",
    "@types/jest": "^29.2.0",
    "@types/node": "^18.11.0",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/react-linkify": "^1.0.1",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "@vitejs/plugin-react": "^4.0.1",
    "circular-dependency-plugin": "^5.2.2",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "dependency-cruiser": "^12.6.0",
    "dotenv": "^16.0.3",
    "electron": "^25.1.1",
    "eslint": "^8.25.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-boundaries": "^3.0.0",
    "eslint-plugin-i18next": "^6.0.0-2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.6",
    "file-loader": "^6.2.0",
    "husky": "^8.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.2.0",
    "jest-environment-jsdom": "^29.2.0",
    "jest-html-reporters": "^3.0.11",
    "loki": "^0.30.3",
    "mini-css-extract-plugin": "^2.7.6",
    "node-loader": "^2.0.0",
    "reg-cli": "^0.17.7",
    "sass": "^1.55.0",
    "sass-loader": "^13.1.0",
    "style-loader": "^3.3.1",
    "stylelint": "^14.14.0",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-config-standard-scss": "^5.0.0",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "ts-morph": "^17.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "vite": "^4.3.9",
    "vite-plugin-svgr": "^3.2.0",
    "webpack": "^5.88.1",
    "webpack-bundle-analyzer": "^4.9.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.5",
    "axios": "^1.3.4",
    "electron-context-menu": "^3.6.1",
    "electron-squirrel-startup": "^1.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-linkify": "^1.0.0-alpha",
    "react-router-dom": "^6.4.2",
    "socket.io-client": "^4.5.4",
    "swr": "^2.1.4",
    "update-electron-app": "^2.0.1",
    "web-vitals": "^3.1.1",
    "zustand": "^4.3.3"
  }
}
