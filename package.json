{
  "name": "lalocord",
  "version": "1.4.2",
  "description": "P2P streaming and chating",
  "author": {
    "name": "Nikita Ozhegov",
    "email": "nikita.ozhegov8@mail.ru"
  },
  "main": ".webpack/main",
  "repository": "github:Kaptoiiika/lalocord",
  "scripts": {
    "start": "vite --mode development",
    "build": "vite build --emptyOutDir",
    "electron": "electron-forge start",
    "package": "electron-forge package",
    "publish": "electron-forge publish",
    "make": "electron-forge make",
    "lint": "npm run lint:ts && npm run lint:scss",
    "lint:fix": "npm run lint:ts:fix && npm run lint:scss:fix",
    "lint:ts": "npx eslint \"**/*{ts,tsx}\"",
    "lint:ts:fix": "npx eslint \"**/*{ts,tsx}\" --fix",
    "lint:scss": "npx stylelint \"**/*.scss\"",
    "lint:scss:fix": "npx stylelint \"**/*.scss\" --fix",
    "storybook": "start-storybook -p 6006 -c ./config/storybook",
    "build-storybook": "build-storybook -c ./config/storybook",
    "test:unit": "jest --config ./config/jest/jest.config.ts",
    "test:ui": "npx loki test --port 6006 ",
    "test:ui:ci": "npm run build-storybook && npx loki --requireReference --reactUri file:./storybook-static",
    "test:ui:ok": "npx loki approve",
    "test:ui:report": "npm run test:ui:report:json && npm run test:ui:report:html",
    "test:ui:report:json": "node scripts/generate-visual-json-report.js",
    "test:ui:report:html": "npx reg-cli --from .loki/report.json --report .loki/report.html",
    "prepare": "husky install",
    "dependency": "npx depcruise --config ./scripts/.dependency-cruiser.js src"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.8.0",
    "@electron-forge/maker-deb": "^7.8.0",
    "@electron-forge/maker-rpm": "^7.8.0",
    "@electron-forge/maker-squirrel": "^7.8.0",
    "@electron-forge/maker-zip": "^7.8.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.8.0",
    "@electron-forge/plugin-vite": "^7.8.0",
    "@electron-forge/plugin-webpack": "^7.8.0",
    "@electron-forge/publisher-github": "^7.8.0",
    "@feature-sliced/eslint-config": "^0.1.1",
    "@storybook/addon-actions": "^8.6.10",
    "@storybook/addon-essentials": "^8.6.10",
    "@storybook/addon-interactions": "^8.6.10",
    "@storybook/addon-links": "^8.6.10",
    "@storybook/react": "^8.6.10",
    "@storybook/testing-library": "^0.2.2",
    "@svgr/webpack": "^8.1.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/circular-dependency-plugin": "^5.0.8",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.14",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "@types/react-linkify": "^1.0.4",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "@vitejs/plugin-react": "^4.3.4",
    "circular-dependency-plugin": "^5.2.2",
    "copy-webpack-plugin": "^13.0.0",
    "css-loader": "^7.1.2",
    "dependency-cruiser": "^16.10.0",
    "dotenv": "^16.4.7",
    "electron": "^35.1.2",
    "eslint": "^9.23.0",
    "eslint-import-resolver-typescript": "^4.2.5",
    "eslint-plugin-boundaries": "^5.0.1",
    "eslint-plugin-i18next": "^6.1.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-storybook": "^0.12.0",
    "file-loader": "^6.2.0",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-html-reporters": "^3.1.7",
    "loki": "^0.35.1",
    "mini-css-extract-plugin": "^2.9.2",
    "node-loader": "^2.1.0",
    "reg-cli": "^0.18.10",
    "sass": "^1.86.0",
    "sass-loader": "^16.0.5",
    "style-loader": "^4.0.0",
    "stylelint": "^16.17.0",
    "stylelint-config-standard": "^37.0.0",
    "stylelint-config-standard-scss": "^14.0.0",
    "ts-jest": "^29.3.0",
    "ts-loader": "^9.5.2",
    "ts-morph": "^25.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2",
    "vite": "^6.2.3",
    "vite-plugin-svgr": "^4.3.0",
    "webpack": "^5.98.0",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.0.0",
    "@mui/material": "^7.0.0",
    "axios": "^1.8.4",
    "electron-context-menu": "^4.0.5",
    "electron-squirrel-startup": "^1.0.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-linkify": "^1.0.0-alpha",
    "react-router-dom": "^7.4.0",
    "socket.io-client": "^4.8.1",
    "swr": "^2.3.3",
    "update-electron-app": "^3.1.1",
    "web-vitals": "^4.2.4",
    "zustand": "^5.0.3"
  }
}
